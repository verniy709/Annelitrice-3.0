			<options>
				<li>
					<weight>10</weight>
					<only>(1,1,1)</only>
				</li>
			</options>
		</colorGenerator>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li>
				 <compClass>CompQuality</compClass>
			</li>
			<li>
				 <compClass>CompColorable</compClass>
			</li>
		</comps>
	</ThingDef>

	<ThingDef Name="Anneli_ArmTest_ApparelCommonBase" ParentName="Anneli_ArmTest_ApparelSpecialBase" Abstract="True">
		<apparel>
      		<tags>
				<li>Anneli</li>
				<li>EVA</li>
			</tags>
		</apparel>
	</ThingDef>
	
</Defs>