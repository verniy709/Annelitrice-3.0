<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="OutfitStandBase">
    <defName>Anneli_OutfitStand</defName>
    <label>annelitrice outfit stand</label>
    <description>Annelitrice outfit stand.Powered by NareisLib, it supports multilayer rendering and features an interaction logic different from the vanilla outfit stand: Exchange All Apparel.</description>
    <thingClass>NareisLib.Building_NewOutfitStand</thingClass>
    <costStuffCount>35</costStuffCount>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Anneli_Building/OutfitStand_BuildingIcon</texPath>
      <drawSize>(1.3, 1.3)</drawSize>
    </graphicData>
    <building>
      <storageGroupTag>OutfitStand</storageGroupTag>
      <defaultStorageSettings Inherit="False">
        <priority>Important</priority>
        <filter>
          <categories>
            <li>Anneli_Category_Transmog</li>
            <li MayRequire="Verniy709.Annelitrice3.0.Ahmislan">Anneli_Category_Ahmislan</li>
            <li MayRequire="Verniy709.Annelitrice3.0.Gakuen">Anneli_Category_Gakuen</li>
            <li MayRequire="Verniy709.Annelitrice3.0.Jurnizia">Anneli_Category_Jurnizia</li>
            <li MayRequire="Verniy709.Annelitrice3.0.NeoYokohama">Anneli_Category_NeoYokohama</li>
            <li MayRequire="Verniy709.Annelitrice3.0.YoRHa">Anneli_Category_YoRHa</li>
          </categories>
          <disallowedCategories>
            <li>ApparelUtility</li>
            <li>Weapons</li>
          </disallowedCategories>
        </filter>
      </defaultStorageSettings>
    </building>
    <inspectorTabs Inherit="False">
      <li>ITab_Storage</li>
      <li>NareisLib.ITab_ContentsNewOutfitStand</li>
    </inspectorTabs>
    <modExtensions>
      <li Class="NareisLib.OutfitStandRenderPlanExtension">
        <renderPlanDef>Annelitrice</renderPlanDef>
        <defaultGender>Female</defaultGender>
        <standOffset>(0,0,0.05)</standOffset>
        <swapAllApparelButtonIconPath>Anneli_Building/OutfitStand_SwapApparelGizmoIcon</swapAllApparelButtonIconPath>
        <raceOffsets>
          <li>
            <raceDefName>Annelitrice</raceDefName>
            <offset>(0,0,0.47)</offset>
            <bodyTexPath>Anneli_Building/OutfitStand_Body</bodyTexPath>
            <bodyTexPathMale>Anneli_Building/OutfitStand_Body</bodyTexPathMale>
            <bodyTexPathFemale>Anneli_Building/OutfitStand_Body</bodyTexPathFemale>
            <headTexPath>Anneli_Building/OutfitStand_Head</headTexPath>
            <headTexPathMale>Anneli_Building/OutfitStand_Head</headTexPathMale>
            <headTexPathFemale>Anneli_Building/OutfitStand_Head</headTexPathFemale>
          </li>
        </raceOffsets>
      </li>
    </modExtensions>
  </ThingDef>
</Defs>
